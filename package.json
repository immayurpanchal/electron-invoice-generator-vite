{
  "name": "electron-invoice-generator-vite",
  "version": "2.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate.",
  "author": "Mayur Panchal",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "format": "prettier --write \"src/**/*.+(ts|tsx|js|jsx|json|html|css|scss|md|yml|yaml|mdx)\"",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@prisma/migrate": "^3.15.2",
    "antd": "^5.3.1",
    "autoprefixer": "^10.4.14",
    "electron-updater": "^5.3.0",
    "nanoid": "^4.0.2",
    "postcss": "^8.4.21",
    "prisma": "^4.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.0",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^23.1.1",
    "electron-builder": "^24.1.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-electron-plugin": "^0.8.2",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
