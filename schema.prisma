datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model products {
  id            Int    @id @default(autoincrement())
  product_name  String
  qty           Int
  product_price Float
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
  output        = "./resources"
}
